compositor_node MainWorkspace_Node
{
    in 0 renderwindow

    target renderwindow
    {
        pass clear
        {
            colour_value 0.2 0 0.2 1
        }
        pass render_scene
        {
            material_scheme ShaderGeneratorDefaultScheme
            rq_first 0
            rq_last  max
        }
    }
}

workspace MainWorkspace
{
    connect_output MainWorkspace_Node 0
}
